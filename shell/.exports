# Algorithms 4 setting
if [ -z "$CLASSPATH" ]
then
  export CLASSPATH=~/algs4/algs4.jar
else
  export CLASSPATH=$CLASSPATH:~/algs4/algs4.jar
fi

export RSYNC_LOCAL_DIR="$(echo ~)/Personal/rsync-folder/"
export RSYNC_CLOUD_DIR='/home/ubuntu/Personal/rsync-folder/'
export NVM_LAZY_LOAD=true # lozy load nvm npm node
export ZSH_PLUGINS_ARRAY=(
  git
  git-extras
  git-flow
  mercurial
  autojump
  colored-man
  colorize
  github
  pip
  python
  zsh-syntax-highlighting
  docker
  docker-compose
  node
  sudo
  zsh-nvm # provide lazy load for nvm
  # nvm
  # npm
  # django
  # web-search
  # vagrant
)

# Functions export
[ $(uname -s | grep -c CYGWIN) -eq 1 ] && OS_NAME="CYGWIN" || OS_NAME=`uname -s`